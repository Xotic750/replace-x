    "@babel/plugin-syntax-export-namespace-from": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-property-mutators": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@prorenata/eslint-config-vue": "^1.5.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-js-logger": "^1.0.17",
    "babel-plugin-lodash": "^3.3.4",
    "caniuse-lite": "^1.0.30000889",
    "chalk": "^2.4.1",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
