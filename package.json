    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsdoc": "^3.8.0",
    "eslint-plugin-json": "^1.2.1",
